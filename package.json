{
  "name": "vue-import-loader",
  "version": "1.0.0",
  "description": "Webpack loader to automatically detect & import used components",
  "keywords": [
    "webpack",
    "vue",
    "loader",
    "components",
    "import"
  ],
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "jest --bail --findRelatedTests"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/privatenumber/vue-import-loader.git"
  },
  "author": "Hiroki Osame <hiroki.osame@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/privatenumber/vue-import-loader/issues"
  },
  "homepage": "https://github.com/privatenumber/vue-import-loader#readme",
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.6",
    "memfs": "^3.2.0",
    "outdent": "^0.7.1",
    "unionfs": "^4.4.0",
    "vue": "^2.6.11",
    "vue-loader": "^15.9.2",
    "webpack": "^4.43.0"
  },
  "dependencies": {
    "acorn": "^7.2.0",
    "acorn-jsx": "^5.2.0",
    "acorn-walk": "^7.1.1",
    "loader-utils": "^2.0.0",
    "vue-template-compiler": "^2.6.11"
  }
}
